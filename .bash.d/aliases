## Over-rides
alias rm="rm -i"
alias cp="cp --no-preserve=mode"
alias dirs='dirs -v | sed "s;$_projects;;g"'


## BABS aliases
alias ,,ts="make -f ${_lab}/working/time/makefile"


## Personal aliases
alias ,config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'
alias ,cdw="cd $_working"
alias ,duh="du -d 1 -h | sort -h"
alias ,mm=micromamba

alias ,r=alias_r
alias ,shell=alias_shell




